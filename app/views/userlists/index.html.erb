<h1>Listing userlists</h1>

<table>
  <tr>
    <th>Uid</th>
    <th>Permissionid</th>
  </tr>

<% @userlists.each do |userlist| %>
  <tr>
    <td><%=h userlist.uid %></td>
    <td><%=h userlist.permissionid %></td>
    <td><%= link_to 'Show', userlist %></td>
    <td><%= link_to 'Edit', edit_userlist_path(userlist) %></td>
    <td><%= link_to 'Destroy', userlist, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New userlist', new_userlist_path %>