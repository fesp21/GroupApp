<%= render :file => 'groups/header.html.erb' %>
<div id="maincontent-box">
	<div id="maincontent-head">
		<strong><%=h @group.name %> - Conference</strong>
	</div>
	<div id="maincontent-body">
		<object type="application/x-shockwave-flash" data="http://me.tokbox.com/vc/<%=conferences[@group.id]%>/10" width="700" height="500">
			<param name="movie" value="http://me.tokbox.com/vc/<%=conferences[@group.id]%>/10" />
			<param name="allowFullScreen" value="true" />
			<param name="allowScriptAccess" value="always" />
			<param name="flashvars" value="guestList=true&textChat=true&displayName=<%=current_user.username%>&showSocialInvite=false&observerMode=true&showInviteButton=false" />
		</object>
	</div>
</div>