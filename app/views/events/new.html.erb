<h1>New event</h1>

<% form_for(@event) do |f| %>
  <%= f.error_messages %>
  <p>
	<%= f.label :name%>
	<%= f.text_field :name%>
  </p>
  <p>
	<%= f.label :start_at%>
	<%= f.datetime_select :start_at%>
  </p>
  <p>
	<%= f.label :end_at%>
	<%= f.datetime_select :end_at%>
  </p>
  <p>
	<%= f.label :all_day %><br />
	<%= f.check_box :all_day %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', events_path %>