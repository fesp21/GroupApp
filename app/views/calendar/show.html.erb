<%= render :file => 'groups/header.html.erb' %>
<div id="maincontent-box">
	<div id="maincontent-head">
		<strong><%=h @group.name %> - Calendar</strong>
	</div>
	<div id="maincontent-body">
		<!-- Probably move the stylesheet to you layout. Also make sure you include the javascript. -->
		<%= stylesheet_link_tag "event_calendar" %>
		<%= javascript_include_tag "event_calendar.js" %>
		<%= javascript_include_tag "prototype" %>
		<h1>Calendar</h1>

		<%= event_calendar %>
	</div>
</div>