<h1>Editing membership</h1>

<% form_for(@membership) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :group_id %><br />
    <%= f.text_field :group_id %>
  </p>
  <p>
	<%= f.label :established %><br />
	<%= f.check_box :established %>
  </p>
  <p>
	<%= f.label :request %><br />
	<%= f.check_box :request %>
  </p>
  <p>
	<%= f.label :invitation %><br />
	<%= f.check_box :invitation %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @membership %> |
<%= link_to 'Back', memberships_path %>