<h1>New newsfeed</h1>

<% form_for(@newsfeed) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :descriptions %><br />
    <%= f.text_area :descriptions %>
  </p>
  <p>
    <%= f.label :time %><br />
    <%= f.datetime_select :time %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', newsfeeds_path %>